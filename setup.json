{
    "name": "aiida-restapi",
    "author": "The AiiDA Team",
    "author_email": "leopold.talirz@gmail.com",
    "description": "AiiDA REST API for data queries and workflow managment.",
    "url": "https://github.com/aiidateam/aiida-restapi",
    "license": "MIT",
    "classifiers": [
        "Programming Language :: Python",
        "Intended Audience :: Science/Research",
        "License :: OSI Approved :: MIT License",
        "Natural Language :: English",
        "Framework :: AiiDA"
    ],
    "version": "0.1.0a0",
    "entry_points": {
        "aiida.data": [
            "restapi = aiida_restapi.data:DiffParameters"
        ],
        "aiida.cmdline.data": [
            "restapi = aiida_restapi.cli:data_cli"
        ]
    },
    "include_package_data": true,
    "setup_requires": ["reentry"],
    "reentry_register": true,
    "python_requires": ">=3.7,<4",
    "install_requires": [
        "aiida-core>=1.6,<2.0.0",
        "sqlalchemy<1.4",
        "fastapi~=0.65.1",
        "uvicorn[standard]>=0.12.0,<0.14.0",
        "pydantic~=1.8.2",
        "pydantic-jsonapi==0.11.0"
    ],
    "extras_require": {
        "testing": [
            "pgtest~=1.3.1",
            "wheel~=0.31",
            "coverage",
            "pytest~=3.6,<5.0.0",
            "pytest-regressions",
            "pytest-cov",
            "requests"
        ],
        "pre-commit": [
            "pre-commit~=2.12"
        ],
        "docs": [
            "sphinx",
            "sphinxcontrib-contentui",
            "sphinxcontrib-details-directive; python_version>='3.0'",
            "sphinx-rtd-theme"
        ],
        "auth": [
            "python-jose",
            "python-multipart",
            "passlib"
        ]
    }
}
